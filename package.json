{
  "name": "github-org-stats",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src test --ext jsx,js",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "import-data": "npm run get-data && npm run generate-index",
    "get-data": "node management/get-data.js > public/data/`date -u --iso-8601=minutes | cut -d '+' -f 1`.json",
    "generate-index": "node management/create-index.js > src/assets/list.json"
  },
  "author": "Jirka Chadima <chadima.jiri@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "reactstrap": "^6.5.0"
  },
  "devDependencies": {
    "node-fetch": "^2.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
